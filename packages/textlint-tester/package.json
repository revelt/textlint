{
  "name": "textlint-tester",
  "version": "12.1.0",
  "description": "testing tool for textlint rule.",
  "keywords": [
    "test",
    "testing",
    "textlint"
  ],
  "homepage": "https://github.com/textlint/textlint/tree/master/packages/textlint-tester/",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "module/",
    "src/",
    "!*.tsbuildinfo"
  ],
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint.git"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib/ out/",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*-test.{js,ts}\"",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@textlint/feature-flag": "^12.1.0",
    "@textlint/kernel": "^12.1.0",
    "textlint": "^12.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.18.0",
    "analyze-desumasu-dearu": "^4.0.1",
    "cross-env": "^7.0.3",
    "match-index": "^1.0.3",
    "mocha": "^8.4.0",
    "rimraf": "^3.0.2",
    "textlint-plugin-html": "^0.2.0",
    "textlint-rule-helper": "^2.2.1",
    "textlint-rule-max-number-of-lines": "^1.0.3",
    "textlint-rule-no-todo": "^2.0.1",
    "ts-node": "^10.4.0",
    "ts-node-test-register": "^10.0.0",
    "typescript": "~4.4.4"
  },
  "email": "azuciao@gmail.com"
}
